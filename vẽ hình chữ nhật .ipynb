    "import turtle \n",
    "\n",
    "t = turtle.Turtle()\n",
    "\n",
    "m = 200;\n",
    "n = 400;\n",
    "\n",
    "t.goto(-100,-200)\n",
    "t.pensize(10)\n",
    "\n",
    "t.forward(m) \n",
    "t.left(90)\n",
    "\n",
    "t.forward(n)\n",
    "t.left(90)\n",
    "\n",
    "t.forward(m)\n",
    "t.left (90)\n",
    "\n",
    "t.forward (n)\n",
    "t.left (90)\n",
    "\n",
    "turtle.done()"
